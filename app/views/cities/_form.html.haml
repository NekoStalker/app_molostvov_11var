= form_for @city do |f|
  - if @city.errors.any?
    #error_explanation
      %h2= "#{pluralize(@city.errors.count, "error")} prohibited this city from being saved:"
      %ul
        - @city.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :cname
    = f.text_field :cname
  .form-actions.bottom10
    = f.submit nil, class: 'btn btn-primary'
